<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.adobe.business.dao.CategoryDao">
    <resultMap id="Category" type="org.adobe.business.pojo.Category">
        <id column="category_id" jdbcType="INTEGER" property="categoryId"/>
        <result column="category_name" jdbcType="VARCHAR" property="categoryName"/>
        <result column="category_dec" jdbcType="VARCHAR" property="categoryDec"/>
    </resultMap>

    <select id="selectAll"  resultMap="Category">
        select * from t_category
    </select>
</mapper>